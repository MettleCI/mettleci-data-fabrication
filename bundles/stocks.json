    "categories": [
        {
            "id": "stock",
            "name": "Stock",
            "generators": [
                {
                    "id": "exchange_code",
                    "name": "Exchange code",
                    "description": "Exchange code",
                    "type": "STRING",
                    "expression": "template('stocks.exchange.code')"
                },
                {
                    "id": "exchange_name",
                    "name": "Exchange name",
                    "description": "Exchange name",
                    "type": "STRING",
                    "expression": "template('stocks.exchange.name')"
                },
                {
                    "id": "exchange_mic",
                    "name": "Exchange MIC",
                    "description": "Exchange Market Identifier Code",
                    "type": "STRING",
                    "expression": "template('stocks.exchange.mic')"
                },
                {
                    "id": "stock_symbol",
                    "name": "Stock symbol",
                    "description": "Stock symbol for the company",
                    "type": "STRING",
                    "parameters": [
                        {
                        "id": "exchange",
                        "name": "Exchange",
                        "description": "Exchange upon which the stock is listed",
                        "type": "STRING",
                        "nullable": true,
                        "editor": {
                            "type": "SelectList",
                            "values": {
                            "lse": "LSE",
                            "nsdq": "NSDQ",
                            "nyse": "NYSE"
                            }
                        }
                    }
                    ],
                    "expression": "template('stocks.':NullToValue(params.exchange,'all'):'.symbol')"
                },
                {
                    "id": "name",
                    "name": "Stock name",
                    "description": "Stock name",
                    "type": "STRING",
                    "parameters": [
                        {
                        "id": "exchange",
                        "name": "Exchange",
                        "description": "Exchange upon which the stock is listed",
                        "type": "STRING",
                        "nullable": true,
                        "editor": {
                            "type": "SelectList",
                            "values": {
                            "lse": "LSE",
                            "nsdq": "NSDQ",
                            "nyse": "NYSE"
                            }
                        }
                    }
                    ],
                    "expression": "template('stocks.':NullToValue(params.exchange,'all'):'.name')"
                },
                {
                    "id": "price",
                    "name": "Stock price",
                    "description": "Sale price (use for latest, high, low)",
                    "type": "STRING",
                    "expression": "regex('\\$[1-9]([0-9]{0,2})\\.([0-9]{2})')"
                },
                {
                    "id": "net_change",
                    "name": "Net change",
                    "description": "Change in stock price from previous close",
                    "type": "STRING",
                    "expression": "regex('[+-]\\$[1-9]([0-9]{0,1})\\.([0-9]{2})')"
                },
                {
                    "id": "pct_change",
                    "name": "Percent change",
                    "description": "Percentage change in stock price",
                    "type": "STRING",
                    "expression": "regex('[+-][0-9]\\.([0-9]{2})%')"
                },
                {
                    "id": "market_cap",
                    "name": "Market cap",
                    "description": "Market capitalization",
                    "type": "STRING",
                    "expression": "regex('\\$([1-9][0-9]{9,12})')"
                },
                {
                    "id": "ipo_year",
                    "name": "IPO year",
                    "description": "Year of initial public offering",
                    "type": "STRING",
                    "expression": "YearFromDate(DateFromDaysSince(Abs(Mod(Random(),20000)),'1950-01-01'))"
                },
                {
                    "id": "analyst_rating",
                    "name": "Analyst rating",
                    "description": "Analyst rating for the stock",
                    "type": "STRING",
                    "expression": "template('stocks.analyst_rating')"
                },
                {
                    "id": "volume",
                    "name": "Volume",
                    "description": "Number of shares traded",
                    "type": "INTEGER",
                    "expression": "10000+Abs(Mod(Random(),1000000))"
                },
                {
                    "id": "year_volume",
                    "name": "Year volume traded",
                    "description": "Total volume of shares traded in the year",
                    "type": "INTEGER",
                    "expression": "1000000+Abs(Mod(Random(),100000000))"

                }
            ]
        }
    ]
}







